[32m+[m[32musing System;[m
[32m+[m
[32m+[m[32mnamespace ASP_NETRest[m
[32m+[m[32m{[m
[32m+[m[32m    public class WeatherForecast[m
[32m+[m[32m    {[m
[32m+[m[32m        public DateTime Date { get; set; }[m
[32m+[m
[32m+[m[32m        public int TemperatureC { get; set; }[m
